<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- State Checked -->
    <item android:state_checked="true">
        <layer-list>
            <item android:width="16dp" android:height="16dp" android:drawable="@drawable/baseline_check_24" android:gravity="center" />


        </layer-list>
    </item>

    <!-- State Unchecked -->
    <item android:state_checked="false">
        <layer-list>
            <item android:width="16dp" android:height="16dp" android:drawable="@drawable/baseline_close_24" android:gravity="center" />
        </layer-list>
    </item>
</selector>